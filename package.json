{
  "name": "clean-ts-api",
  "version": "1.1.0",
  "description": "NodeJs Rest API using TDD, Clean Architecture and Typescript",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc",
    "dev": "sucrase-node src/main/server.ts --trace-warnings",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache  --testTimeout",
    "test:verbose": "jest --passWithNoTests",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "author": "Joao Pedro Goes",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mongodb": "^3.6.12",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-commit-msg-linter": "^2.3.11",
    "husky": "^4.0.6",
    "jest": "^26.6.3",
    "lint-staged": "^9.5.0",
    "rimraf": "^3.0.2",
    "sucrase": "^3.17.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.1",
    "validator": "^13.5.2"
  }
}
